<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>org.engyne.node-api</string>
    <key>ProgramArguments</key>
    <array>
      <string>__ROOT_DIR__/scripts/node_run.sh</string>
    </array>
    <key>WorkingDirectory</key>
    <string>__ROOT_DIR__</string>
    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key>
      <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
      <key>ENV_FILE</key>
      <string>__ROOT_DIR__/.env.node</string>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>__ROOT_DIR__/logs/node_api.log</string>
    <key>StandardErrorPath</key>
    <string>__ROOT_DIR__/logs/node_api.err.log</string>
  </dict>
</plist>
